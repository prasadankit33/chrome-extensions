      
	{
  "manifest_version": 2,
  "name": "Ankit Background script Extension 2",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]         //scripts used for activity outside the webpage present on browser 
		},									//executed by going to chrome://extensions and go to your extension
										   //and click background page in it.
    "browser_action" : {
  },
  "browser_action": {
    "default_icon": "background.png"
  }
}